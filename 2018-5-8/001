#include <iostream>
using namespace std;
int main()
{
    string s1("Welcome ");
    s1.append("To C++");
    cout << s1 <<endl ;
    
    string s2("Welcome ");
    s2.append("To C And C++",0,5);
    cout << s2 <<endl ;
    
    string s3("Welcome ");
    s3.append("To C++",4);
    cout << s3 <<endl ;
    
    string s4("Welcome ");
    s4.append(6,'G');
    cout << s4 <<endl ;

  return 0;
}
